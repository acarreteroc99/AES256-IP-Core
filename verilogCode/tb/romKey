#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x560ab2f2ebc0 .scope module, "tb_mod_romKey" "tb_mod_romKey" 2 7;
 .timescale -9 -11;
P_0x560ab2f1db40 .param/l "addr_width" 0 2 11, +C4<00000000000000000000000000000100>;
P_0x560ab2f1db80 .param/l "data_width" 0 2 10, +C4<00000000000000000000000010000000>;
P_0x560ab2f1dbc0 .param/l "period" 1 2 9, +C4<00000000000000000000000000110010>;
v0x560ab2f48ba0_0 .var "addr", 3 0;
v0x560ab2f48c60_0 .var "clk", 0 0;
v0x560ab2f48d30_0 .net "data", 127 0, v0x560ab2f1e600_0;  1 drivers
v0x560ab2f48e30_0 .net "done", 0 0, v0x560ab2f1e9c0_0;  1 drivers
v0x560ab2f48f00_0 .var "resetn", 0 0;
v0x560ab2f48ff0_0 .var "startBit", 0 0;
v0x560ab2f490c0_0 .var "wr_en", 0 0;
S_0x560ab2ef60a0 .scope module, "DUT" "mod_romKey" 2 21, 3 7 0, S_0x560ab2f2ebc0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "resetn"
    .port_info 2 /INPUT 1 "startBit"
    .port_info 3 /INPUT 4 "addr"
    .port_info 4 /INPUT 1 "wr_en"
    .port_info 5 /OUTPUT 128 "data"
    .port_info 6 /OUTPUT 1 "done"
P_0x560ab2f07a50 .param/l "addr_width" 0 3 19, +C4<00000000000000000000000000000100>;
P_0x560ab2f07a90 .param/l "data_width" 0 3 18, +C4<00000000000000000000000010000000>;
v0x560ab2f1df40_0 .net "addr", 3 0, v0x560ab2f48ba0_0;  1 drivers
v0x560ab2f1e200_0 .net "clk", 0 0, v0x560ab2f48c60_0;  1 drivers
v0x560ab2f1e600_0 .var "data", 127 0;
v0x560ab2f1e9c0_0 .var "done", 0 0;
v0x560ab2f1ed40_0 .net "resetn", 0 0, v0x560ab2f48f00_0;  1 drivers
v0x560ab2f1eec0 .array "rom", 15 0, 127 0;
v0x560ab2f09b60_0 .net "startBit", 0 0, v0x560ab2f48ff0_0;  1 drivers
v0x560ab2f48810_0 .net "wr_en", 0 0, v0x560ab2f490c0_0;  1 drivers
E_0x560ab2f2b820 .event negedge, v0x560ab2f1e200_0;
E_0x560ab2f2b930 .event posedge, v0x560ab2f1e200_0;
E_0x560ab2f2b430/0 .event negedge, v0x560ab2f1ed40_0;
E_0x560ab2f2b430/1 .event posedge, v0x560ab2f1e200_0;
E_0x560ab2f2b430 .event/or E_0x560ab2f2b430/0, E_0x560ab2f2b430/1;
S_0x560ab2f489b0 .scope task, "test_rom" "test_rom" 2 36, 2 36 0, S_0x560ab2f2ebc0;
 .timescale -9 -11;
TD_tb_mod_romKey.test_rom ;
    %wait E_0x560ab2f2b930;
    %vpi_call 2 39 "$display", "TEST - The corresponding value for address %h is %h \012", v0x560ab2f48ba0_0, v0x560ab2f48d30_0 {0 0 0};
    %end;
    .scope S_0x560ab2ef60a0;
T_1 ;
    %vpi_call 3 24 "$readmemh", "/home/adrian/Desktop/AES256-HW-Accelerator/rijndaelTables/key.txt", v0x560ab2f1eec0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x560ab2ef60a0;
T_2 ;
    %wait E_0x560ab2f2b430;
    %load/vec4 v0x560ab2f1ed40_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ab2f1e9c0_0, 0, 1;
    %pushi/vec4 0, 0, 128;
    %store/vec4 v0x560ab2f1e600_0, 0, 128;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x560ab2ef60a0;
T_3 ;
    %wait E_0x560ab2f2b930;
    %load/vec4 v0x560ab2f48810_0;
    %load/vec4 v0x560ab2f09b60_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x560ab2f1df40_0;
    %pad/u 6;
    %ix/vec4 4;
    %load/vec4a v0x560ab2f1eec0, 4;
    %assign/vec4 v0x560ab2f1e600_0, 0;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x560ab2f1e9c0_0;
T_3.0 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x560ab2ef60a0;
T_4 ;
    %wait E_0x560ab2f2b820;
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x560ab2f1e9c0_0;
    %jmp T_4;
    .thread T_4;
    .scope S_0x560ab2f2ebc0;
T_5 ;
    %delay 10000, 0;
    %load/vec4 v0x560ab2f48c60_0;
    %nor/r;
    %store/vec4 v0x560ab2f48c60_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x560ab2f2ebc0;
T_6 ;
    %vpi_call 2 31 "$dumpfile", "wv_mod_romKey.vcd" {0 0 0};
    %vpi_call 2 32 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x560ab2f2ebc0 {0 0 0};
    %end;
    .thread T_6;
    .scope S_0x560ab2f2ebc0;
T_7 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ab2f48c60_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ab2f48f00_0, 0, 1;
    %wait E_0x560ab2f2b930;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ab2f48ff0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ab2f48f00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ab2f490c0_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x560ab2f48ba0_0, 0, 4;
    %fork TD_tb_mod_romKey.test_rom, S_0x560ab2f489b0;
    %join;
    %wait E_0x560ab2f2b930;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ab2f490c0_0, 0, 1;
    %wait E_0x560ab2f2b930;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ab2f490c0_0, 0, 1;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x560ab2f48ba0_0, 0, 4;
    %fork TD_tb_mod_romKey.test_rom, S_0x560ab2f489b0;
    %join;
    %wait E_0x560ab2f2b930;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ab2f490c0_0, 0, 1;
    %wait E_0x560ab2f2b930;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ab2f490c0_0, 0, 1;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x560ab2f48ba0_0, 0, 4;
    %fork TD_tb_mod_romKey.test_rom, S_0x560ab2f489b0;
    %join;
    %wait E_0x560ab2f2b930;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ab2f490c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ab2f48ff0_0, 0, 1;
    %wait E_0x560ab2f2b930;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ab2f490c0_0, 0, 1;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x560ab2f48ba0_0, 0, 4;
    %fork TD_tb_mod_romKey.test_rom, S_0x560ab2f489b0;
    %join;
    %wait E_0x560ab2f2b930;
    %wait E_0x560ab2f2b930;
    %vpi_call 2 84 "$finish" {0 0 0};
    %end;
    .thread T_7;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_mod_romKey.sv";
    "./../design/mod_romKey.sv";
