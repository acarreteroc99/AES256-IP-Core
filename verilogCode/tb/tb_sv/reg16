v0x562b7283bce0_0 .net "resetn", 0 0, v0x562b7283c330_0;  1 drivers
E_0x562b7281cc50/0 .event negedge, v0x562b7283bce0_0;
E_0x562b7281cc50/1 .event posedge, v0x562b727fba40_0;
E_0x562b7281cc50 .event/or E_0x562b7281cc50/0, E_0x562b7281cc50/1;
    .scope S_0x562b727e70a0;
T_0 ;
    %wait E_0x562b7281cc50;
    %load/vec4 v0x562b7283bce0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x562b7283bb00_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x562b7283bb00_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.3, 5;
    %pushi/vec4 0, 0, 8;
    %load/vec4 v0x562b7283bb00_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x562b7283bbf0_0, 4, 8;
    %load/vec4 v0x562b7283bb00_0;
    %addi 1, 0, 32;
    %store/vec4 v0x562b7283bb00_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x562b7283bb00_0, 0, 32;
T_0.4 ;
    %load/vec4 v0x562b7283bb00_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.5, 5;
    %load/vec4 v0x562b7280f020_0;
    %load/vec4 v0x562b7283bb00_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x562b7283bb00_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x562b7283bbf0_0, 4, 8;
    %load/vec4 v0x562b7283bb00_0;
    %addi 1, 0, 32;
    %store/vec4 v0x562b7283bb00_0, 0, 32;
    %jmp T_0.4;
T_0.5 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x562b7281ed30;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562b7283bf50_0, 0, 1;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562b7283bf50_0, 0, 1;
    %delay 2000, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x562b7281ed30;
T_2 ;
    %pushi/vec4 0, 0, 8;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x562b7283c040_0, 4, 8;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x562b7283c170_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x562b7283c170_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_2.1, 5;
    %load/vec4 v0x562b7283c040_0;
    %load/vec4 v0x562b7283c170_0;
    %subi 1, 0, 32;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %addi 1, 0, 8;
    %load/vec4 v0x562b7283c170_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x562b7283c040_0, 4, 8;
    %load/vec4 v0x562b7283c170_0;
    %addi 1, 0, 32;
    %store/vec4 v0x562b7283c170_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %end;
    .thread T_2;
    .scope S_0x562b7281ed30;
T_3 ;
    %wait E_0x562b7281cc50;
    %vpi_call 2 45 "$display", "This is the resetn test" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562b7283c330_0, 0, 1;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x562b7283c170_0, 0, 32;
T_3.0 ;
    %load/vec4 v0x562b7283c170_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_3.1, 5;
    %delay 2000, 0;
    %load/vec4 v0x562b7283be70_0;
