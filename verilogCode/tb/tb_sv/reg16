    %muli 8, 0, 35;
    %part/s 8;
    %cmpi/e 0, 0, 8;
    %jmp/0xz  T_3.2, 4;
    %load/vec4 v0x562b7283be70_0;
    %load/vec4 v0x562b7283c170_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 52 "$display", "Correct value in position %i \012", S<0,vec4,u8> {1 0 0};
    %jmp T_3.3;
T_3.2 ;
    %load/vec4 v0x562b7283be70_0;
    %load/vec4 v0x562b7283c170_0;
    %pad/s 35;
    %muli 8, 0, 35;
