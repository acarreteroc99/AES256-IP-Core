 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "resetn"
    .port_info 2 /INPUT 128 "i"
    .port_info 3 /OUTPUT 128 "o"
P_0x562b727e7220 .param/l "N" 1 3 13, +C4<00000000000000000000000000010000>;
v0x562b727fba40_0 .net "clk", 0 0, v0x562b7283bf50_0;  1 drivers
v0x562b7280f020_0 .net "i", 127 0, v0x562b7283c040_0;  1 drivers
