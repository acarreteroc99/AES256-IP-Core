T_4 ;
    %wait E_0x562b7281c6c0;
    %vpi_call 2 61 "$display", "This is the storage test" {0 0 0};
    %delay 100000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x562b7283c170_0, 0, 32;
T_4.0 ;
    %load/vec4 v0x562b7283c170_0;
