    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x562b7283c040_0;
    %load/vec4 v0x562b7283c170_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %cmp/e;
    %jmp/0xz  T_4.2, 4;
    %load/vec4 v0x562b7283c210_0;
    %load/vec4 v0x562b7283c170_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 67 "$display", "Correct value: %h \012", S<0,vec4,u8> {1 0 0};
