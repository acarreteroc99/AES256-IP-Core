    %load/vec4 v0x562b7283c170_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x562b7283c210_0;
    %load/vec4 v0x562b7283c170_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 69 "$display", "Expected value: %h ;; Value written: %h ;; Pos: %h \012", S<1,vec4,u8>, S<0,vec4,u8>, v0x562b7283c170_0 {2 0 0};
T_4.3 ;
    %load/vec4 v0x562b7283c170_0;
    %addi 1, 0, 32;
    %store/vec4 v0x562b7283c170_0, 0, 32;
