T_2.0 ;
    %load/vec4 v0x55fd8ef5ef30_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_2.1, 5;
    %load/vec4 v0x55fd8ef5ec30_0;
    %load/vec4 v0x55fd8ef5ef30_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x55fd8ef5efd0_0;
    %load/vec4 v0x55fd8ef5ef30_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %cmp/e;
    %jmp/0xz  T_2.2, 4;
    %load/vec4 v0x55fd8ef5ec30_0;
    %load/vec4 v0x55fd8ef5ef30_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x55fd8ef5efd0_0;
    %load/vec4 v0x55fd8ef5ef30_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 102 "$display", "Aux value: %h ;; Output value: %h \012", S<1,vec4,u8>, S<0,vec4,u8> {2 0 0};
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0x55fd8ef5ec30_0;
