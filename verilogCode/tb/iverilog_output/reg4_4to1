TD_tb_mod_reg4_1to4.enableResetn ;
    %wait E_0x55fd8ef33a80;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fd8ef5f290_0, 0, 1;
