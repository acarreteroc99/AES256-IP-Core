#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x564c1bd3aa10 .scope module, "tb_mod_regCTRL" "tb_mod_regCTRL" 2 8;
 .timescale -9 -11;
P_0x564c1bd14ce0 .param/l "N" 1 2 10, +C4<00000000000000000000000000000101>;
P_0x564c1bd14d20 .param/l "period" 1 2 11, +C4<00000000000000000000000000010100>;
v0x564c1bd53310_0 .var "clk", 0 0;
v0x564c1bd53400_0 .var "pos", 4 0;
v0x564c1bd534d0_0 .var "read", 0 0;
v0x564c1bd535d0_0 .var "resetn", 0 0;
v0x564c1bd536a0_0 .var "value", 0 0;
S_0x564c1bd030a0 .scope module, "DUT" "mod_regCTRL" 2 20, 3 8 0, S_0x564c1bd3aa10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "resetn"
    .port_info 2 /INPUT 1 "read"
    .port_info 3 /INPUT 5 "pos"
    .port_info 4 /INPUT 1 "value"
P_0x564c1bd03270 .param/l "N" 1 3 12, +C4<00000000000000000000000000000101>;
v0x564c1bd2a4d0_0 .net "clk", 0 0, v0x564c1bd53310_0;  1 drivers
v0x564c1bd2a7e0_0 .var "flags", 4 0;
v0x564c1bd2ab50_0 .var/i "index", 31 0;
v0x564c1bd17460_0 .net "pos", 4 0, v0x564c1bd53400_0;  1 drivers
v0x564c1bd16df0_0 .net "read", 0 0, v0x564c1bd534d0_0;  1 drivers
v0x564c1bd52cf0_0 .net "resetn", 0 0, v0x564c1bd535d0_0;  1 drivers
v0x564c1bd52db0_0 .net "value", 0 0, v0x564c1bd536a0_0;  1 drivers
E_0x564c1bd38eb0/0 .event edge, v0x564c1bd52cf0_0, v0x564c1bd2ab50_0, v0x564c1bd16df0_0, v0x564c1bd17460_0;
E_0x564c1bd38eb0/1 .event edge, v0x564c1bd52db0_0, v0x564c1bd2a7e0_0;
E_0x564c1bd38eb0 .event/or E_0x564c1bd38eb0/0, E_0x564c1bd38eb0/1;
S_0x564c1bd52f10 .scope task, "enableRead" "enableRead" 2 45, 2 45 0, S_0x564c1bd3aa10;
 .timescale -9 -11;
E_0x564c1bd3bb20 .event posedge, v0x564c1bd2a4d0_0;
TD_tb_mod_regCTRL.enableRead ;
    %vpi_call 2 47 "$display", "Enabling read signal" {0 0 0};
    %wait E_0x564c1bd3bb20;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564c1bd534d0_0, 0, 1;
    %wait E_0x564c1bd3bb20;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564c1bd534d0_0, 0, 1;
    %end;
S_0x564c1bd53120 .scope task, "enableResetn" "enableResetn" 2 36, 2 36 0, S_0x564c1bd3aa10;
 .timescale -9 -11;
E_0x564c1bd3b050 .event negedge, v0x564c1bd2a4d0_0;
TD_tb_mod_regCTRL.enableResetn ;
    %wait E_0x564c1bd3bb20;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564c1bd535d0_0, 0, 1;
    %wait E_0x564c1bd3b050;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564c1bd535d0_0, 0, 1;
    %end;
    .scope S_0x564c1bd030a0;
T_2 ;
    %wait E_0x564c1bd38eb0;
    %load/vec4 v0x564c1bd52cf0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x564c1bd2ab50_0, 0, 32;
T_2.2 ;
    %load/vec4 v0x564c1bd2ab50_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_2.3, 5;
    %pushi/vec4 0, 0, 1;
    %ix/getv/s 4, v0x564c1bd2ab50_0;
    %store/vec4 v0x564c1bd2a7e0_0, 4, 1;
    %load/vec4 v0x564c1bd2ab50_0;
    %addi 1, 0, 32;
    %store/vec4 v0x564c1bd2ab50_0, 0, 32;
    %jmp T_2.2;
T_2.3 ;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x564c1bd16df0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.4, 8;
    %vpi_call 3 29 "$display", "Position of the array is: %d", v0x564c1bd17460_0 {0 0 0};
    %load/vec4 v0x564c1bd52db0_0;
    %ix/getv 4, v0x564c1bd17460_0;
    %store/vec4 v0x564c1bd2a7e0_0, 4, 1;
    %vpi_call 3 31 "$display", "The value of the flags reg rn is: %b", v0x564c1bd2a7e0_0 {0 0 0};
T_2.4 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x564c1bd3aa10;
T_3 ;
    %delay 10000, 0;
    %load/vec4 v0x564c1bd53310_0;
    %nor/r;
    %store/vec4 v0x564c1bd53310_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x564c1bd3aa10;
T_4 ;
    %vpi_call 2 30 "$dumpfile", "wv_mod_regCTRL.vcd" {0 0 0};
    %vpi_call 2 31 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x564c1bd3aa10 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x564c1bd3aa10;
T_5 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564c1bd53310_0, 0, 1;
    %fork TD_tb_mod_regCTRL.enableResetn, S_0x564c1bd53120;
    %join;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564c1bd534d0_0, 0, 1;
    %wait E_0x564c1bd3bb20;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564c1bd536a0_0, 0, 1;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x564c1bd53400_0, 0, 5;
    %wait E_0x564c1bd3bb20;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564c1bd536a0_0, 0, 1;
    %pushi/vec4 3, 0, 5;
    %store/vec4 v0x564c1bd53400_0, 0, 5;
    %wait E_0x564c1bd3bb20;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564c1bd536a0_0, 0, 1;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x564c1bd53400_0, 0, 5;
    %delay 2000, 0;
    %vpi_call 2 108 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
