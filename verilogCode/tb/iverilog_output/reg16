v0x560cf996df60_0 .net "wr_en", 0 0, v0x560cf996f0b0_0;  1 drivers
E_0x560cf9948110 .event posedge, v0x560cf996dea0_0, v0x560cf99376d0_0;
S_0x560cf996e0e0 .scope task, "enableResetn" "enableResetn" 2 39, 2 39 0, S_0x560cf994b0c0;
 .timescale -9 -11;
E_0x560cf9948620 .event posedge, v0x560cf99376d0_0;
TD_tb_mod_reg16.enableResetn ;
    %wait E_0x560cf9948620;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560cf996efe0_0, 0, 1;
    %wait E_0x560cf9948620;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560cf996efe0_0, 0, 1;
    %end;
S_0x560cf996e2f0 .scope task, "enableWrite" "enableWrite" 2 49, 2 49 0, S_0x560cf994b0c0;
 .timescale -9 -11;
TD_tb_mod_reg16.enableWrite ;
    %vpi_call 2 51 "$display", "Enabling read signal" {0 0 0};
    %wait E_0x560cf9948620;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560cf996f0b0_0, 0, 1;
    %wait E_0x560cf9948620;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560cf996f0b0_0, 0, 1;
    %end;
S_0x560cf996e4c0 .scope task, "setInput" "setInput" 2 59, 2 59 0, S_0x560cf994b0c0;
 .timescale -9 -11;
v0x560cf996e690_0 .var "seed", 7 0;
TD_tb_mod_reg16.setInput ;
    %load/vec4 v0x560cf996e690_0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560cf996ec20_0, 4, 8;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x560cf996ed20_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x560cf996ed20_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_2.1, 5;
    %load/vec4 v0x560cf996ec20_0;
    %load/vec4 v0x560cf996ed20_0;
    %subi 1, 0, 32;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %addi 1, 0, 8;
    %load/vec4 v0x560cf996ed20_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x560cf996ec20_0, 4, 8;
    %load/vec4 v0x560cf996ed20_0;
    %addi 1, 0, 32;
    %store/vec4 v0x560cf996ed20_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %end;
S_0x560cf996e770 .scope task, "test_read" "test_read" 2 86, 2 86 0, S_0x560cf994b0c0;
 .timescale -9 -11;
TD_tb_mod_reg16.test_read ;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x560cf996ed20_0, 0, 32;
T_3.2 ;
    %load/vec4 v0x560cf996ed20_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_3.3, 5;
    %delay 2000, 0;
    %load/vec4 v0x560cf996ec20_0;
    %load/vec4 v0x560cf996ed20_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x560cf996edf0_0;
    %load/vec4 v0x560cf996ed20_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %cmp/e;
    %jmp/0xz  T_3.4, 4;
    %load/vec4 v0x560cf996edf0_0;
    %load/vec4 v0x560cf996ed20_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 93 "$display", "Correct value: %h \012", S<0,vec4,u8> {1 0 0};
    %jmp T_3.5;
T_3.4 ;
    %load/vec4 v0x560cf996ec20_0;
    %load/vec4 v0x560cf996ed20_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x560cf996edf0_0;
    %load/vec4 v0x560cf996ed20_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 95 "$display", "Expected value: %h ;; Value read: %h ;; Pos: %d \012", S<1,vec4,u8>, S<0,vec4,u8>, v0x560cf996ed20_0 {2 0 0};
T_3.5 ;
    %load/vec4 v0x560cf996ed20_0;
    %addi 1, 0, 32;
    %store/vec4 v0x560cf996ed20_0, 0, 32;
    %jmp T_3.2;
T_3.3 ;
    %end;
S_0x560cf996e990 .scope task, "test_resetn" "test_resetn" 2 68, 2 68 0, S_0x560cf994b0c0;
 .timescale -9 -11;
TD_tb_mod_reg16.test_resetn ;
    %load/vec4 v0x560cf996efe0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.6, 8;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x560cf996ed20_0, 0, 32;
T_4.8 ;
    %load/vec4 v0x560cf996ed20_0;
    %cmpi/s 16, 0, 32;
