v0x560cf996efe0_0 .var "resetn", 0 0;
v0x560cf996f0b0_0 .var "wr_en", 0 0;
S_0x560cf99120a0 .scope module, "DUT" "mod_reg16" 2 21, 3 8 0, S_0x560cf994b0c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "resetn"
    .port_info 2 /INPUT 1 "wr_en"
    .port_info 3 /INPUT 128 "i"
