S_0x560cf994b0c0 .scope module, "tb_mod_reg16" "tb_mod_reg16" 2 8;
 .timescale -9 -11;
P_0x560cf9954d60 .param/l "N" 1 2 10, +C4<00000000000000000000000000010000>;
P_0x560cf9954da0 .param/l "period" 1 2 11, +C4<00000000000000000000000000010100>;
v0x560cf996eb60_0 .var "clk", 0 0;
v0x560cf996ec20_0 .var "i", 127 0;
v0x560cf996ed20_0 .var/i "index", 31 0;
v0x560cf996edf0_0 .net "o", 127 0, v0x560cf996dca0_0;  1 drivers
