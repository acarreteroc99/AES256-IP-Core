    %vpi_call 2 93 "$display", "Correct value: %h \012", S<0,vec4,u8> {1 0 0};
    %jmp T_3.5;
T_3.4 ;
    %load/vec4 v0x56318c92f120_0;
    %load/vec4 v0x56318c92f220_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x56318c92f2f0_0;
    %load/vec4 v0x56318c92f220_0;
    %pad/s 35;
    %muli 8, 0, 35;
