    .scope S_0x560cf99120a0;
T_6 ;
    %wait E_0x560cf9948110;
    %load/vec4 v0x560cf996dea0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560cf996dd90_0, 0, 1;
