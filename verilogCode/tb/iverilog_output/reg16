S_0x56318c90bbb0 .scope module, "tb_mod_reg16" "tb_mod_reg16" 2 8;
 .timescale -9 -11;
P_0x56318c915490 .param/l "N" 1 2 10, +C4<00000000000000000000000000010000>;
