#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55d3e9993d10 .scope module, "tb_mod_reg16" "tb_mod_reg16" 2 8;
 .timescale -9 -11;
P_0x55d3e996db40 .param/l "N" 1 2 10, +C4<00000000000000000000000000010000>;
P_0x55d3e996db80 .param/l "period" 1 2 11, +C4<00000000000000000000000000010100>;
v0x55d3e99b4c00_0 .var "clk", 0 0;
v0x55d3e99b4ca0_0 .var "i", 127 0;
v0x55d3e99b4da0_0 .var/i "index", 31 0;
v0x55d3e99b4e70_0 .net "o", 127 0, v0x55d3e99706f0_0;  1 drivers
v0x55d3e99b4f70_0 .var "read", 0 0;
v0x55d3e99b5060_0 .var "resetn", 0 0;
S_0x55d3e995c0a0 .scope module, "DUT" "mod_reg16" 2 20, 3 8 0, S_0x55d3e9993d10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "resetn"
    .port_info 2 /INPUT 1 "read"
    .port_info 3 /INPUT 128 "i"
    .port_info 4 /OUTPUT 128 "o"
P_0x55d3e995c270 .param/l "N" 1 3 13, +C4<00000000000000000000000000010000>;
v0x55d3e9970a50_0 .net "clk", 0 0, v0x55d3e99b4c00_0;  1 drivers
v0x55d3e9970d60_0 .net "i", 127 0, v0x55d3e99b4ca0_0;  1 drivers
v0x55d3e99703e0_0 .var/i "index", 31 0;
v0x55d3e99706f0_0 .var "o", 127 0;
v0x55d3e9987510_0 .net "read", 0 0, v0x55d3e99b4f70_0;  1 drivers
v0x55d3e99b42f0_0 .net "resetn", 0 0, v0x55d3e99b5060_0;  1 drivers
E_0x55d3e9991e90 .event posedge, v0x55d3e9970a50_0;
S_0x55d3e99b4450 .scope task, "enableRead" "enableRead" 2 49, 2 49 0, S_0x55d3e9993d10;
 .timescale -9 -11;
TD_tb_mod_reg16.enableRead ;
    %vpi_call 2 51 "$display", "Enabling read signal" {0 0 0};
    %wait E_0x55d3e9991e90;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d3e99b4f70_0, 0, 1;
    %wait E_0x55d3e9991e90;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d3e99b4f70_0, 0, 1;
    %end;
S_0x55d3e99b4640 .scope task, "enableResetn" "enableResetn" 2 40, 2 40 0, S_0x55d3e9993d10;
 .timescale -9 -11;
TD_tb_mod_reg16.enableResetn ;
    %wait E_0x55d3e9991e90;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d3e99b5060_0, 0, 1;
    %wait E_0x55d3e9991e90;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d3e99b5060_0, 0, 1;
    %end;
S_0x55d3e99b4810 .scope task, "test_read" "test_read" 2 77, 2 77 0, S_0x55d3e9993d10;
 .timescale -9 -11;
TD_tb_mod_reg16.test_read ;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d3e99b4da0_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x55d3e99b4da0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_2.1, 5;
    %delay 2000, 0;
    %load/vec4 v0x55d3e99b4ca0_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x55d3e99b4e70_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %cmp/e;
    %jmp/0xz  T_2.2, 4;
    %load/vec4 v0x55d3e99b4e70_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 84 "$display", "Correct value: %h \012", S<0,vec4,u8> {1 0 0};
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0x55d3e99b4ca0_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x55d3e99b4e70_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 86 "$display", "Expected value: %h ;; Value read: %h ;; Pos: %d \012", S<1,vec4,u8>, S<0,vec4,u8>, v0x55d3e99b4da0_0 {2 0 0};
T_2.3 ;
    %load/vec4 v0x55d3e99b4da0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d3e99b4da0_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %vpi_call 2 88 "$stop" {0 0 0};
    %end;
S_0x55d3e99b49e0 .scope task, "test_resetn" "test_resetn" 2 59, 2 59 0, S_0x55d3e9993d10;
 .timescale -9 -11;
TD_tb_mod_reg16.test_resetn ;
    %load/vec4 v0x55d3e99b5060_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.4, 8;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d3e99b4da0_0, 0, 32;
T_3.6 ;
    %load/vec4 v0x55d3e99b4da0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_3.7, 5;
    %delay 2000, 0;
    %load/vec4 v0x55d3e99b4e70_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %cmpi/e 0, 0, 8;
    %jmp/0xz  T_3.8, 4;
    %load/vec4 v0x55d3e99b4e70_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 68 "$display", "Correct value in position %d \012", S<0,vec4,u8> {1 0 0};
    %jmp T_3.9;
T_3.8 ;
    %load/vec4 v0x55d3e99b4e70_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %vpi_call 2 70 "$display", "Something not working properly. Value: %h ; Pos: %d \012", S<0,vec4,u8>, v0x55d3e99b4da0_0 {1 0 0};
T_3.9 ;
    %load/vec4 v0x55d3e99b4da0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d3e99b4da0_0, 0, 32;
    %jmp T_3.6;
T_3.7 ;
T_3.4 ;
    %vpi_call 2 73 "$stop" {0 0 0};
    %end;
    .scope S_0x55d3e995c0a0;
T_4 ;
    %wait E_0x55d3e9991e90;
    %load/vec4 v0x55d3e99b42f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d3e99703e0_0, 0, 32;
T_4.2 ;
    %load/vec4 v0x55d3e99703e0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.3, 5;
    %pushi/vec4 0, 0, 8;
    %load/vec4 v0x55d3e99703e0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x55d3e99706f0_0, 4, 8;
    %load/vec4 v0x55d3e99703e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d3e99703e0_0, 0, 32;
    %jmp T_4.2;
T_4.3 ;
    %jmp T_4.1;
T_4.0 ;
    %load/vec4 v0x55d3e9987510_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.4, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d3e99703e0_0, 0, 32;
T_4.6 ;
    %load/vec4 v0x55d3e99703e0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.7, 5;
    %load/vec4 v0x55d3e9970d60_0;
    %load/vec4 v0x55d3e99703e0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x55d3e99703e0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x55d3e99706f0_0, 4, 8;
    %load/vec4 v0x55d3e99703e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d3e99703e0_0, 0, 32;
    %jmp T_4.6;
T_4.7 ;
T_4.4 ;
T_4.1 ;
    %jmp T_4;
    .thread T_4;
    .scope S_0x55d3e9993d10;
T_5 ;
    %delay 10000, 0;
    %load/vec4 v0x55d3e99b4c00_0;
    %nor/r;
    %store/vec4 v0x55d3e99b4c00_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x55d3e9993d10;
T_6 ;
    %vpi_call 2 31 "$dumpfile", "wv_mod_reg16.vcd" {0 0 0};
    %vpi_call 2 32 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d3e9993d10 {0 0 0};
    %pushi/vec4 0, 0, 8;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55d3e99b4ca0_0, 4, 8;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x55d3e99b4da0_0, 0, 32;
T_6.0 ;
    %load/vec4 v0x55d3e99b4da0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_6.1, 5;
    %load/vec4 v0x55d3e99b4ca0_0;
    %load/vec4 v0x55d3e99b4da0_0;
    %subi 1, 0, 32;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %addi 1, 0, 8;
    %load/vec4 v0x55d3e99b4da0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x55d3e99b4ca0_0, 4, 8;
    %load/vec4 v0x55d3e99b4da0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d3e99b4da0_0, 0, 32;
    %jmp T_6.0;
T_6.1 ;
    %end;
    .thread T_6;
    .scope S_0x55d3e9993d10;
T_7 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d3e99b4c00_0, 0, 1;
    %fork TD_tb_mod_reg16.enableResetn, S_0x55d3e99b4640;
    %join;
    %fork TD_tb_mod_reg16.test_resetn, S_0x55d3e99b49e0;
    %join;
    %fork TD_tb_mod_reg16.enableRead, S_0x55d3e99b4450;
    %join;
    %fork TD_tb_mod_reg16.test_read, S_0x55d3e99b4810;
    %join;
    %vpi_call 2 99 "$finish" {0 0 0};
    %end;
    .thread T_7;
# The file index is used to find the file name in the following table.
