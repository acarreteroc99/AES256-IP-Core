    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %load/vec4 v0x56318c92f2f0_0;
    %load/vec4 v0x56318c92f220_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %part/s 8;
    %cmp/e;
    %jmp/0xz  T_3.4, 4;
    %load/vec4 v0x56318c92f2f0_0;
    %load/vec4 v0x56318c92f220_0;
