    .port_info 5 /OUTPUT 1 "reg_full"
P_0x560cf9912270 .param/l "N" 1 3 13, +C4<00000000000000000000000000010000>;
v0x560cf99376d0_0 .net "clk", 0 0, v0x560cf996eb60_0;  1 drivers
v0x560cf99266d0_0 .net "i", 127 0, v0x560cf996ec20_0;  1 drivers
v0x560cf996dbe0_0 .var/i "index", 31 0;
v0x560cf996dca0_0 .var "o", 127 0;
v0x560cf996dd90_0 .var "reg_full", 0 0;
