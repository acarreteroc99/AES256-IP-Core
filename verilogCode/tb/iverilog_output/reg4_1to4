S_0x55eef6807cc0 .scope module, "tb_mod_reg4_1to4" "tb_mod_reg4_1to4" 2 8;
 .timescale -9 -11;
P_0x55eef67f96d0 .param/l "N" 1 2 10, +C4<00000000000000000000000000000100>;
