    .scope S_0x55eef67ce0a0;
T_6 ;
    %wait E_0x55eef6803cc0;
    %load/vec4 v0x55eef682ab60_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.0, 8;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x55eef67e2eb0_0, 0, 3;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55eef682a8b0_0, 0, 32;
T_6.2 ;
    %load/vec4 v0x55eef682a8b0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_6.3, 5;
    %pushi/vec4 0, 0, 8;
    %load/vec4 v0x55eef682a8b0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
    %store/vec4 v0x55eef67f3710_0, 4, 8;
    %pushi/vec4 0, 0, 8;
    %load/vec4 v0x55eef682a8b0_0;
    %pad/s 35;
    %muli 8, 0, 35;
    %ix/vec4/s 4;
