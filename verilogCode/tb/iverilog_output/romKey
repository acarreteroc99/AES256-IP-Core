#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55680eef25b0 .scope module, "tb_mod_romKey" "tb_mod_romKey" 2 7;
 .timescale -9 -11;
P_0x55680eef2730 .param/l "addr_width" 0 2 11, +C4<00000000000000000000000000000100>;
P_0x55680eef2770 .param/l "data_width" 0 2 10, +C4<00000000000000000000000010000000>;
P_0x55680eef27b0 .param/l "period" 1 2 9, +C4<00000000000000000000000000110010>;
v0x55680ef085c0_0 .var "addr", 3 0;
v0x55680ef086a0_0 .var "clk", 0 0;
v0x55680ef08740_0 .net "data", 127 0, v0x55680ef07dc0_0;  1 drivers
v0x55680ef087e0_0 .net "done", 0 0, v0x55680ef07e80_0;  1 drivers
v0x55680ef08880_0 .var "wr_en", 0 0;
S_0x55680eebb0a0 .scope module, "DUT" "mod_romKey" 2 19, 3 7 0, S_0x55680eef25b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 4 "addr"
    .port_info 2 /INPUT 1 "wr_en"
    .port_info 3 /OUTPUT 128 "data"
    .port_info 4 /OUTPUT 1 "done"
P_0x55680eecc9c0 .param/l "addr_width" 0 3 19, +C4<00000000000000000000000000000100>;
P_0x55680eecca00 .param/l "data_width" 0 3 18, +C4<00000000000000000000000010000000>;
v0x55680eebb360_0 .net "addr", 3 0, v0x55680ef085c0_0;  1 drivers
v0x55680ef07d00_0 .net "clk", 0 0, v0x55680ef086a0_0;  1 drivers
v0x55680ef07dc0_0 .var "data", 127 0;
v0x55680ef07e80_0 .var "done", 0 0;
v0x55680ef07f40 .array "rom", 15 0, 127 0;
v0x55680ef08250_0 .net "wr_en", 0 0, v0x55680ef08880_0;  1 drivers
v0x55680ef07f40_0 .array/port v0x55680ef07f40, 0;
v0x55680ef07f40_1 .array/port v0x55680ef07f40, 1;
E_0x55680eef0830/0 .event edge, v0x55680ef08250_0, v0x55680eebb360_0, v0x55680ef07f40_0, v0x55680ef07f40_1;
v0x55680ef07f40_2 .array/port v0x55680ef07f40, 2;
v0x55680ef07f40_3 .array/port v0x55680ef07f40, 3;
v0x55680ef07f40_4 .array/port v0x55680ef07f40, 4;
v0x55680ef07f40_5 .array/port v0x55680ef07f40, 5;
E_0x55680eef0830/1 .event edge, v0x55680ef07f40_2, v0x55680ef07f40_3, v0x55680ef07f40_4, v0x55680ef07f40_5;
v0x55680ef07f40_6 .array/port v0x55680ef07f40, 6;
v0x55680ef07f40_7 .array/port v0x55680ef07f40, 7;
v0x55680ef07f40_8 .array/port v0x55680ef07f40, 8;
v0x55680ef07f40_9 .array/port v0x55680ef07f40, 9;
E_0x55680eef0830/2 .event edge, v0x55680ef07f40_6, v0x55680ef07f40_7, v0x55680ef07f40_8, v0x55680ef07f40_9;
v0x55680ef07f40_10 .array/port v0x55680ef07f40, 10;
v0x55680ef07f40_11 .array/port v0x55680ef07f40, 11;
v0x55680ef07f40_12 .array/port v0x55680ef07f40, 12;
v0x55680ef07f40_13 .array/port v0x55680ef07f40, 13;
E_0x55680eef0830/3 .event edge, v0x55680ef07f40_10, v0x55680ef07f40_11, v0x55680ef07f40_12, v0x55680ef07f40_13;
v0x55680ef07f40_14 .array/port v0x55680ef07f40, 14;
v0x55680ef07f40_15 .array/port v0x55680ef07f40, 15;
E_0x55680eef0830/4 .event edge, v0x55680ef07f40_14, v0x55680ef07f40_15;
E_0x55680eef0830 .event/or E_0x55680eef0830/0, E_0x55680eef0830/1, E_0x55680eef0830/2, E_0x55680eef0830/3, E_0x55680eef0830/4;
S_0x55680ef083b0 .scope task, "test_rom" "test_rom" 2 34, 2 34 0, S_0x55680eef25b0;
 .timescale -9 -11;
E_0x55680eef0bb0 .event posedge, v0x55680ef07d00_0;
TD_tb_mod_romKey.test_rom ;
    %wait E_0x55680eef0bb0;
    %vpi_call 2 37 "$display", "TEST - The corresponding value for address %h is %h \012", v0x55680ef085c0_0, v0x55680ef08740_0 {0 0 0};
    %end;
    .scope S_0x55680eebb0a0;
T_1 ;
    %vpi_call 3 23 "$readmemh", "/home/adrian/Desktop/AES256-HW-Accelerator/rijndaelTables/key.txt", v0x55680ef07f40 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55680eebb0a0;
T_2 ;
    %wait E_0x55680eef0830;
    %load/vec4 v0x55680ef08250_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %load/vec4 v0x55680eebb360_0;
    %pad/u 6;
    %ix/vec4 4;
    %load/vec4a v0x55680ef07f40, 4;
    %assign/vec4 v0x55680ef07dc0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55680ef07e80_0, 0;
T_2.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55680ef07e80_0, 0;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x55680eef25b0;
T_3 ;
    %delay 10000, 0;
    %load/vec4 v0x55680ef086a0_0;
    %nor/r;
    %store/vec4 v0x55680ef086a0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55680eef25b0;
T_4 ;
    %vpi_call 2 29 "$dumpfile", "wv_mod_romKey.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55680eef25b0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x55680eef25b0;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55680ef086a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55680ef08880_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55680ef085c0_0, 0, 4;
    %fork TD_tb_mod_romKey.test_rom, S_0x55680ef083b0;
    %join;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x55680ef085c0_0, 0, 4;
    %fork TD_tb_mod_romKey.test_rom, S_0x55680ef083b0;
    %join;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x55680ef085c0_0, 0, 4;
    %fork TD_tb_mod_romKey.test_rom, S_0x55680ef083b0;
    %join;
    %vpi_call 2 52 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
